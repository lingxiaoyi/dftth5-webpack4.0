{
  "name": "webpack-seed",
  "version": "4.0.0",
  "description": "This is a seed for web application built by webpack",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env=pro --progress --colors --bail",
    "test": "./node_modules/.bin/webpack --env=test --progress --colors --bail",
    "dev": "./node_modules/.bin/webpack --env=dev --progress --colors --config ./webpack.dev.config.js",
    "start": "./node_modules/.bin/webpack-dev-server --env=dev --config ./webpack.dev.config.js --open --hot",
    "profile": "./node_modules/.bin/webpack --colors --profile --display-modules --config ./webpack.dev.config.js",
    "dll": "./node_modules/.bin/webpack --progress --colors --config ./webpack-dll.config.js",
    "analyse": "NODE_ENV=dev webpack --json > ./build/stats.json && webpack-bundle-analyzer --port 8989 ./build/stats.json",
    "analyze": "npm run analyse"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "zhijun",
  "license": "MIT",
